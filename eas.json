      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_NAME": "BETA-GymFlow",
        "APP_ICON": "./assets/images/icon-dev.png",
        "APP_PACKAGE": "brainiac.workout_.dev"
      }
    },
    "preview": {
