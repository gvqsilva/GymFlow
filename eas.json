{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "applicationId": "brainiac.workout_.dev"
      },
      "env": {
        "APP_NAME": "BETA-GymFlow",
        "APP_ICON": "./assets/images/icon-dev.png"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_NAME": "GymFlow",
        "APP_ICON": "./assets/images/icon-gym.png"
      }
    }
  },
  "submit": {
    "production": {}
  }
}